net: "FCN_RNN.prototxt"
test_iter: 100
# make test net, but don't invoke it from the solver itself
test_interval: 2000
display: 20
average_loss: 20
eval_type: "segmentation"
lr_policy: "poly"
# lr for unnormalized softmax -- see train_val definition
base_lr: 1e-13
# high momentum
momentum: 0.99
# no gradient accumulation
iter_size: 8
max_iter: 20000
weight_decay: 0.0005
snapshot: 4000
snapshot_prefix: "snapshots/crf_rnn"
test_initialization: false
#debug_info: true